<% if flash[:notice] %> 
  <%= flash[:notice] %> 
<% end %>
<h1>Please insert Username and Password to Log in.</h1>
<%= form_for(@user, url: users_login_check_path) do |f|%>
	<%= f.label :username %>
	<%= f.text_field :username %>

	<%= f.label :password %>
	<%= f.text_field :password %>

	<%= f.submit %>

<% end %>
	